    {
      "cell_type": "code",
      "source": [
        "# для перевода строчных значений в числа:\n",
        "def convert_str_to_float(s_in):\n",
        "  if type(s_in) == type(int()) or type(s_in) == type(float()) or s_in.replace('.','',1).isdigit():\n",
        "    return float(s_in)\n",
        "  else:\n",
        "    # print(s_in)\n",
        "    return None\n",
        "\n",
        "for i in data_8.columns:\n",
        "  data_8[f'{i}'] = data_8[f'{i}'].apply(convert_str_to_float)\n",
        "data_8.info()\n",
        "# теперь можно смотреть статистику"
      ],
